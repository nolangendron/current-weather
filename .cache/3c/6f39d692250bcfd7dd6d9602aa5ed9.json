urce":"Details.js","original":{"line":73,"column":15}},{"generated":{"line":93,"column":41},"source":"Details.js","original":{"line":73,"column":56}},{"generated":{"line":94,"column":0},"source":"Details.js","original":{"line":74,"column":2}},{"generated":{"line":94,"column":9},"source":"Details.js","original":{"line":75,"column":4}},{"generated":{"line":94,"column":38},"source":"Details.js","original":{"line":75,"column":5}},{"generated":{"line":94,"column":60},"source":"Details.js","original":{"line":75,"column":4}},{"generated":{"line":94,"column":68},"source":"Details.js","original":{"line":76,"column":6}},{"generated":{"line":94,"column":97},"source":"Details.js","original":{"line":76,"column":7}},{"generated":{"line":94,"column":104},"source":"Details.js","original":{"line":76,"column":6}},{"name":"props","generated":{"line":94,"column":106},"source":"Details.js","original":{"line":76,"column":19}},{"generated":{"line":94,"column":111},"source":"Details.js","original":{"line":76,"column":6}},{"generated":{"line":94,"column":112},"source":"Details.js","original":{"line":75,"column":4}},{"generated":{"line":94,"column":113},"source":"Details.js","original":{"line":74,"column":2}},{"generated":{"line":95,"column":0},"source":"Details.js","original":{"line":79,"column":1}}],"sources":{"Details.js":"import React from \"react\";\nimport pet from \"@frontendmasters/pet\";\nimport { navigate } from \"@reach/router\";\nimport Modal from \"./Modal\";\nimport Carousel from \"./Carousel\";\nimport ErrorBoundary from \"./ErrorBoundary\";\nimport ThemeContext from \"./ThemeContext\";\n\nclass Details extends React.Component {\n  state = { loading: true, showModal: false };\n  componentDidMount() {\n    pet.animal(this.props.id).then(({ animal }) => {\n      this.setState({\n        url: animal.url,\n        name: animal.name,\n        animal: animal.type,\n        location: `${animal.contact.address.city}, ${animal.contact.address.state}`,\n        description: animal.description,\n        media: animal.photos,\n        breed: animal.breeds.primary,\n        loading: false\n      });\n    }, console.error);\n  }\n  toggleModal = () => this.setState({ showModal: !this.state.showModal });\n  adopt = () => navigate(this.state.url);\n  render() {\n    if (this.state.loading) {\n      return <h1>loading ...</h1>;\n    }\n    const {\n      animal,\n      breed,\n      location,\n      description,\n      name,\n      media,\n      showModal\n    } = this.state;\n\n    return (\n      <div className=\"details\">\n        <Carousel media={media} />\n        <div>\n          <h1>{name}</h1>\n          <h2>{`${animal} - ${breed} - ${location}`}</h2>\n          <ThemeContext.Consumer>\n            {([theme]) => (\n              <button\n                onClick={this.toggleModal}\n                style={{ backgroundColor: theme }}\n              >\n                Adopt {name}\n              </button>\n            )}\n          </ThemeContext.Consumer>\n          <p>{description}</p>\n          {showModal ? (\n            <Modal>\n              <h1>Would you like to adopt {name}?</h1>\n              <div className=\"buttons\">\n                <button onClick={this.adopt}>Yes</button>\n                <button onClick={this.toggleModal}>No</button>\n              </div>\n            </Modal>\n          ) : null}\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default function DetailsWithErrorBoundary(props) {\n  return (\n    <ErrorBoundary>\n      <Details {...props} />\n    </ErrorBoundary>\n  );\n}\n"},"lineCount":null}},"error":null,"hash":"2db59e9efb94611a51919a119d05e3ef","cacheData":{"env":{}}}